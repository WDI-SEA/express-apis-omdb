

<div>

        <% movies.forEach(function(movie) { %>
          <div class="data_titles"><img src="<%= movie.Poster %>">
          <a href="/movies/id/<%= movie.imdbID %>"><%= movie.Title %></a>
          <button imdbID="<%= movie.imdbID %>" class="add_to_favorites">Add to Favorites</button>
          </div>
        <% }) %>

    </div>