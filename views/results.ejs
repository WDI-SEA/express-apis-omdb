<!DOCTYPE html>
<html>
<head>
	<meta charset='utf-8'>
	<title>Movie Results</title>
</head>
<body>
	<h3>Here are your results...</h3>
	<% movies.forEach(function(movie,idx) { %>
		<img src='<%= movie.Poster %>'>
		<a href='/movies/<%=movie.imdbID%>'><h1><%= movie.Title %></h1></a>
		<p><%= movie.Link %></p>
	<% }); %>
</body>
</html>