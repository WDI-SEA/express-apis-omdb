<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= movies.Title %></title>
  </head>
  <body>
    <h1>Movie</h1>
    <% movies.forEach(function(movie){%>
      <img src="<%= movie.Poster %>">
      <li><a href="movie/<%= movie.imdbID %>"><%= movie.Title %>'s Page</a></li>
    <%});%>
  </body>
</html>
