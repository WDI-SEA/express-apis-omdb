<h1><%=`Results for: ${input} ` %></h1>
<% result.Search.forEach(resultItem=>{ %>
    <img src="<%= resultItem.Poster %>" height="250px" width="auto" />

    <p><%=`Title: ${resultItem.Title}, Release Year: ${resultItem.Year}, Media Type: ${resultItem.Type}`%></p>
    <a href="<%=`http://localhost:3001/details?userInput=${resultItem.imdbID}`%>"  >Details</a>
    <br/>
    <br/>
<% }) %>



