<% // Render your search results here %>
<% // This is how to do comments in EJS, by the way... %>
<% movies.forEach(movie => { %>
    <div>
      <h3><%= movie.Title %></h3>
      <p><%= movie.Year %></p>
      <p><%= movie.Type %></p>
      <a href="/details/<%= movie.imdbID %>">more info</a>
    </div>
    <% if (movie.Poster != "N/A") { %>
      <img  src="<%= movie.Poster %>" alt="<%= movie.Title %>'s movie poster"/> 
    <% } %>
  <% }) %>