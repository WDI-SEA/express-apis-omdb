<h2>Your Results</h2>
    <% results.forEach(function(movie) { %>
    <div class="posters col-xs-12 col-sm-4 col-md-3 col-lg-2">
      <a href="/movie/<%= movie.imdbID %>">
        <h3><%= movie.Title %></h3>
      </a>
      <p><%= movie.Year %></p>
      <img src="<%= movie.Poster %>" alt="">
    </div>
    <% }) %>
